{
  "name": "point",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "dev:sqlite": "cp prisma/schema.sqlite.prisma prisma/schema.prisma && prisma generate && next dev",
    "dev:postgres": "cp prisma/schema.postgres.prisma prisma/schema.prisma && prisma generate && next dev",
    "build": "prisma generate && next build",
    "build:railway": "prisma migrate resolve --applied 20251123082909_add_user_blocking && prisma generate && next build",
    "start": "node start.js",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:push:sqlite": "cp prisma/schema.sqlite.prisma prisma/schema.prisma && prisma db push",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "node create-mock-data.js",
    "db:seed:safe": "node -e \"console.log('Seeding if needed...'); try { require('./create-mock-data.js') } catch(e) { console.log('Seed skipped:', e.message) }\"",
    "db:backup": "node scripts/db-backup.js",
    "db:backup:prod": "node scripts/db-backup-prod.js",
    "db:restore": "node scripts/db-restore.js",
    "db:list": "node scripts/db-list.js",
    "db:switch:sqlite": "cp prisma/schema.sqlite.prisma prisma/schema.prisma && prisma generate",
    "db:switch:postgres": "cp prisma/schema.postgres.prisma prisma/schema.prisma && prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.111",
    "@auth/prisma-adapter": "^2.10.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.16.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "d3": "^7.9.0",
    "next": "^15.5.3",
    "next-auth": "^4.24.11",
    "postcss": "^8.5.6",
    "prisma": "^6.16.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "zod": "^4.1.7"
  }
}
